<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:Icon="http://metro.mahapps.com/winfx/xaml/iconpacks">

    <SolidColorBrush x:Key="MainBorder"  Color="#eb6aa3"/>
    
    <LinearGradientBrush x:Key="Gradient" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#fefefe" Offset="0"/>
        <GradientStop Color="#eb6aa3" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="RightBorder"  Color="#eb6aa3"/>
    
    <LinearGradientBrush x:Key="RightGradient" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#fefefe" Offset="0"/>
        <GradientStop Color="#eb6aa3" Offset="1"/>
    </LinearGradientBrush>
    
    <TextBlock x:Key="Map"  Foreground="white"/>

    <Icon:PackIconMaterial x:Key="IconMap" Foreground="white"/>

    <SolidColorBrush x:Key="TabBorder" Color="#fbdfe8"/>

</ResourceDictionary>